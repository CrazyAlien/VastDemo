        </video>
      </div>
      <div id="adContainer"></div>
    </div>
    <button id="playButtonSkip" class="playButton">Play Skippable</button>
    <button id="playButtonAdPod" class="playButton">Ad Pod</button>
    <script type="text/javascript" src="//imasdk.googleapis.com/js/sdkloader/ima3.js"></script>
    <script type="text/javascript" src="javascripts/ads.js"></script>
  </body>
</html>